{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RateLimit": {
    "RefillRate": 1,
    "MaxMessagesPerNumberPerSecond": 2,
    "MaxMessagesPerAccountPerSecond": 10,
    "ResourceExpirationTime": "0:01:00"
  }
}
